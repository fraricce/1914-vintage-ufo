pico-8 cartridge // http://www.pico-8.com
version 21
__lua__
grid_x=0
grid_y=0
map_x = 0
map_tile=0
flag_tile=0
go=true
psprite=0
exploded=false
splashtext = 1

function _init()
 make_player()
end

function _update()

	if (splashtext==1) then
		if (btnp(5)) then
			splashtext=2
		end
		return
	end
	
	if (splashtext==2) then
		if (btnp(5)) then
			splashtext=3
		end
		return
	end	
	
	if (splashtext==3) then
		if (btnp(5)) then
			splashtext=4
		end
		return
	end	
	
	if (splashtext==4) then
		if (btnp(5)) then
			splashtext=-1
			music(1)
		end
		return
	end
		
	if (btn(5) and exploded) then
		make_player()
 	music(1)
		exploded=false
		go=true
		psprite=0
	end
	
	
	grid_x=flr((px+8)/8)
	grid_y=flr((py)/8)
	grid_xx=flr((px+8)/8)
	grid_yy=flr((py+3)/8)
	
 map_tile=mget(grid_x,grid_y)
 map_tilee=mget(grid_xx,grid_yy)
 flag_tile=fget(map_tile)
	flag_tilee=fget(map_tilee)
 
 if map_x<-255 then map_x=0 end
 
 psprite=psprite+1
   
 if not go and not exploded then
 	psprite=49
 	exploded=true
 	music(0)
 end
 	
 if psprite==4 then psprite=0 end
 
 if psprite==63 then 
 	psprite=57 
 end
 
 move_player()
 
 if go then map_x-=1 end
end

function _draw()
 cls()
 map(0,0,map_x,0,16,16)
 map(16,0,map_x+128,0,16,16)
 map(0,0,map_x+256,0,16,16)
 
 title = "1914: vintage ufo.." 
 color(9)
 print(title, hcenter(title),0)
 
 if (splashtext==1) then
 	color(10)
	 print("",0,10)
	 print("\n\nin the year 1914, the world")
	 print("is one step away from a global")
	 print("conflict.\n\nbut...")
	 color(6)
	 print("\n(x to continue)")
 end
 
 if (splashtext==2) then
 	color(10)
	 print("",0,10)
	 print("\n\na night in december,")
	 print("unidentified flying objects")
	 print("appeared in the sky.")
	 color(10)
	 print("\nit was the beginning..")
	 print("of the alien invasion")
	 color(6)
	 print("\n(x to continue)")
 end

 if (splashtext==3) then
 	color(10)
	 print("",0,10)
	 print("\n\nbeing the humankind")
	 print("under the alien menace,")
	 print("all the nations decided")
	 print("to cease any dispute,")
	 print("\nand join forces to face")
	 print("the invaders from space..")

	 color(6)
	 print("\n(x to continue)")
 end
 
  if (splashtext==4) then
 	color(10)
	 print("",0,10)
	 print("\n\nafter tossing a coin,")
	 print("a flying ace called")
	 print("the red baron was drawn")
	 print("to face the enemies.")
	 print("\ngood luck, red baron!")

	 color(6)
	 print("\n(x to continue)")
 end


	if (grid_yy==15) then 
		crs = 'crash! press x'
		print(crs,hcenter(crs),55,10)
		go=false
 end 
	draw_player()
end
function make_player()
 px=64
 py=64
 psprite=1
end
function move_player()
	if not go then return end
 if (btn(0)) px-=1 --left
 if (btn(1)) px+=1 --right
 if (btn(2)) py-=1 --up
 if (btn(3)) py+=1 --down
end
function draw_player()
 spr(psprite,px,py)
end
function hcenter(s)
  -- screen center minus the
  -- string length times the 
  -- pixels in a char's width,
  -- cut in half
  return 64-#s*2
end
